{
  "name": "workspace-ensino",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:cursos": "jest --config ./projects/cursos/jest.config.js --passWithNoTests",
    "test:ensino-commons": "jest  --config ./projects/ensino-commons/jest.config.js --passWithNoTests",
    "test:cursos:coverage": "jest --coverage --config ./projects/cursos/jest.config.js --passWithNoTests",
    "test:ensino-commons:coverage": "jest --coverage --config ./projects/ensino-commons/jest.config.js --passWithNoTests",
    "test": "npm run test:cursos && npm run test:ensino-commons",
    "coverage": "npm run test:cursos:coverage && npm run test:ensino-commons:coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.0",
    "@angular/cdk": "^10.2.6",
    "@angular/common": "~10.1.0",
    "@angular/compiler": "~10.1.0",
    "@angular/core": "~10.1.0",
    "@angular/forms": "~10.1.0",
    "@angular/material": "^10.2.6",
    "@angular/platform-browser": "~10.1.0",
    "@angular/platform-browser-dynamic": "~10.1.0",
    "@angular/router": "~10.1.0",
    "@cca-fab/cca-fab-components-common": "^1.9.0",
    "keycloak-angular": "8.0.1",
    "keycloak-js": "10.0.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.7",
    "@angular/cli": "~10.1.0",
    "@angular/compiler-cli": "~10.1.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.11.1",
    "cz-conventional-changelog": "^3.3.0",
    "codelyzer": "^6.0.0",
    "husky": "^4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^26.6.1",
    "jest-preset-angular": "^8.3.2",
    "ng-packagr": "^10.1.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "ts-jest": "^26.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-angular": "^3.0.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "pretty-quick --staged && ng lint && npm run test"
    }
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ]
  }
}
